class Solution(object):
  def guessNumber(self,n):
    low,high = 1, n
    while low <= high:
      mid = (low +high) // 2
      if guess(mid) == -1: #guess() is offered as an api
        high = mid -1
      elif guess(mid) == 1:
        low = mid - 1
      else:
        return mid

if __name__ == '__main--':
  n = 10
  solution = Solution()
  print(solution.guessNumber(n))

# Time complexity: O(logn)
# Space complexity: O(1)
